{
    "fields": {
        "name": {
            "type": "varchar",
            "required": false,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "selectionExternalSources": {
            "type": "attachmentMultiple",
            "previewSize": "medium",
            "audited": true,
            "sourceList": [],
            "accept": []
        },
        "chunksSummarized": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "readOnly": true,
            "audited": true
        },
        "confidenceScore": {
            "type": "varchar",
            "maxLength": 100,
            "audited": true,
            "options": []
        },
        "costPerOutputToken": {
            "notNull": false,
            "type": "float",
            "readOnly": true,
            "tooltip": true
        },
        "costPerPromptToken": {
            "notNull": false,
            "type": "float",
            "default": 1.71e-6,
            "readOnly": true,
            "tooltip": true
        },
        "coverage": {
            "type": "varchar",
            "maxLength": 200,
            "options": [],
            "tooltip": true
        },
        "freeTextPrompt": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "audited": true,
            "tooltip": true
        },
        "insightVerified": {
            "type": "enum",
            "options": [
                "",
                "approve",
                "review",
                "decline"
            ],
            "style": {
                "": null,
                "approve": "success",
                "review": "warning",
                "decline": "danger"
            },
            "displayAsLabel": true,
            "audited": true,
            "maxLength": 100
        },
        "insightsID": {
            "type": "number",
            "len": 36,
            "notNull": false,
            "unique": false,
            "nextNumber": 1,
            "padLength": 4,
            "prefix": "Insight-"
        },
        "lock": {
            "notNull": true,
            "type": "bool",
            "audited": true
        },
        "modelCompletionTokens": {
            "type": "int",
            "audited": true,
            "readOnly": true
        },
        "modelPromptTokens": {
            "type": "int",
            "audited": true,
            "readOnly": true
        },
        "modelResponse": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "audited": true,
            "readOnly": true,
            "tooltip": true
        },
        "modelTotalTokens": {
            "type": "int",
            "audited": true,
            "readOnly": true
        },
        "numberOfChunks": {
            "type": "int",
            "audited": true,
            "readOnly": true
        },
        "numberOfFormsUsed": {
            "type": "int",
            "audited": true,
            "readOnly": true
        },
        "processingMessage": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "default": "Currently processing input. Please refresh the page again in 2 minutes to see new status.",
            "readOnly": true
        },
        "quotes": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "audited": true
        },
        "recommendedAction": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "audited": true
        },
        "responseGenerationTime": {
            "notNull": false,
            "type": "float",
            "audited": true
        },
        "title": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "rows": 1,
            "audited": true
        },
        "totalCost": {
            "notNull": false,
            "type": "float",
            "readOnly": true,
            "tooltip": true
        },
        "userPromptInstructions": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "default": "[short instruction will come here]",
            "readOnly": true
        },
        "feedbackDatas": {
            "type": "linkMultiple",
            "audited": true,
            "required": true,
            "tooltip": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "selectionExternalSources": {
            "type": "hasChildren",
            "entity": "Attachment",
            "foreign": "parent",
            "layoutRelationshipsDisabled": true,
            "relationName": "attachments",
            "utility": true
        },
        "feedbackDatas": {
            "type": "hasMany",
            "relationName": "cInsightFeedbackData",
            "foreign": "insights",
            "entity": "CFeedbackData",
            "audited": false
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}